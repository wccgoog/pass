<view class="header-container">
  <view 
    class="city"
    onTap="onCityTap"
  >
    <image
      class="map-icon"
      mode="aspectFit"
      src="https://jbxqalipay.nanjingdata.cn/image/map.png" 
    />
    <view 
      class="city-text"
    >
      {{currentArea.name}} 
    </view>

  </view>

</view>