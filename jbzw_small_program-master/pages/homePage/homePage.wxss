}

.b-border {
  display: inline-block;
  height: 80rpx;
}

/* 主题部分 */

.themeWrap {
}

.themeTitle {
  color: #666;
  font-size: 32rpx;
  padding: 20rpx 0 20rpx 20rpx;
  border-bottom: 1px solid #eee;
  display: flex;
}

.themeTitleWords {
  flex: 5;
  padding-left: 20rpx;
  border-left: 4px solid #079a41;
}

.more {
  flex: 1;
  display: flex;
  align-items: center;
}

.rightArrow {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
}

/* 主题分类 */

.themeClassify {
  display: flex;
  flex-flow: row;
  flex-wrap: wrap;
}

.themeSingle {
  width: 50%;
  display: flex;
  align-items: center;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.themeSingle:nth-of-type(odd) {
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
}

.themeSingle:nth-of-type(even) {
  border-bottom: 1px solid #eee;
}

.left {
  flex: 4;
  font-size: 26rpx;
  color: #666;
}

.leftTitle {
}

.leftDetail {
  font-size: 20rpx;
}

.right {

}

.themeSingle .right {
  height: 20px;
  width: 20px;
}

/* 部门 */

.departmentWrap {
  border-top: 10rpx solid #eee;
}

.departmentTitle {
  color: #666;
  font-size: 32rpx;
  padding: 20rpx 0 20rpx 20rpx;
  border-bottom: 1px solid #eee;
  display: flex;
}

.departmentTitleWords {
  flex: 5;
  padding-left: 20rpx;
  border-left: 4px solid #079a41;
}

.departmentClassify {
  display: flex;
  flex-flow: row;
  flex-wrap: wrap;
}

.departmentSingle {
  text-align: center;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  width: 25%;
  color: #666;
  box-sizing: border-box;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
  overflow: hidden;
}

.departmentSingle:nth-of-type(4n+0) {
  border-right: none;
}

.scrollItemHeight {
  height: 60%;
}

/* 轮播图 */

.page-section {
  width: 100%;
}

.page-section-spacing {
  box-sizing: border-box;
}

.swiper-item {
  display: block;
  height: 150px;
}

.swiperHeight {
  height: 375rpx;
}

.apply {
  margin-bottom: 10px;
}

.x {
  color: #999;
  font-size: 12px;
  text-align: center;
  background: white;
  padding-bottom: 10px;
}

.iWanna {
  background: white;
  /* border-bottom: 1px solid #eee; */
  border-left: 4px solid #079a41;
  padding: 5px;
  font-size: 24px;
  color: #444;
}

.avatar {
  height: 35px;
  width: 35px;
  margin: 10px 20px;
  border-radius: 50%;
}

.nickName {
  font-size: 16px;
}

.user {
  display: flex;
  align-items: center;
}

.header {
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 20px;
}

.logout {
  position: absolute;
  bottom: 0;
  vertical-align: middle;
  text-align: center;
  width: 100%;
  border: 1px solid #eee;
  padding: 10px;
  font-size: 40rpx;
  font-weight: 300;
}

.background {
  position: relative;
  width: 100%;
  height: 100px;
  background-image: url('https://jbxqalipay.nanjingdata.cn/image/logo_background.jpg');
  background-position: 50% 30%;
  background-size: cover;
}

.logo {
  position: absolute;
  top: 0;
  right: 0;
  height: 40px;
  width: 120px;
}

.weather {
  color: #eee;
  text-shadow: 1px 1px #000;
  position: absolute;
  bottom: 10px;
  left: 5px;
}

.city {
  font-size: 18px;
  font-weight: 900;
}

.temperature {
  font-size: 11px;
  font-weight: 900;
}
