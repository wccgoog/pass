  top: 0;
  left: 0;
  width: 100%;
}

.headerImg {
  width: 100%;
  vertical-align: bottom;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 50%;
  color: #666;
}

.on {
  color: #333;
  border-bottom: 5rpx solid #079a41;
}

.swiper-box {
  display: block;
